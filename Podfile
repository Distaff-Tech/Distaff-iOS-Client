  pod 'GoogleSignIn'
  pod 'FBSDKLoginKit'
  pod 'FacebookCore'
  pod 'SDWebImage'
  pod 'Firebase/Auth'                 # for Social Integrations
  pod 'SDWebImage'
  pod 'InstagramLogin'
  pod 'EasyToast'                     # for toast
  pod 'SKPhotoBrowser'
  pod 'GrowingTextView'
  pod 'Firebase/Messaging'
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'ScalingCarousel'
